{
  "__viewResolvers": [
    {
      "name": "vm",
      "prefix": "WEB-INF/views",
      "suffix": ".vm",
      "resolverClass": "cc.doctor.framework.web.handler.resolver.modelview.VelocityViewResolver"
    },
    {
      "name":"jsp",
      "prefix":"WEB-INF/views/jsp",
      "suffix": ".jsp",
      "resolverClass": "cc.doctor.framework.web.handler.resolver.modelview.ForwardViewResolver"
    },
    {
      "name":"excel",
      "prefix":"",
      "suffix": ".jsp",
      "resolverClass": "cc.doctor.framework.web.handler.resolver.modelview.ExcelViewResolver"
    }
  ],
  "__default_service_package": "cc.doctor.framework.web.sample",
  "__routes": {
    "/sample": {
      "/view": [
        "SampleService",
        "sampleModelView"
      ],
      "/json": [
        "SampleService",
        "sampleJsonBody"
      ],
      "/jsp": [
        "SampleService",
        "sampleJsp"
      ],
      "/excel.xls": [
        "SampleService",
        "sampleExcel"
      ]
    }
  }
}